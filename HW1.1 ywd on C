#include <stdio.h>

int main()
{
    int day, year, week;

    printf("Days: \n");
    scanf("%d", &day);

    year = (day / 365);
    week = (day % 365) / 7;
    day  = day - ((year * 365) + (week * 7));

    printf("Year: %d\n", year);
    printf("week: %d\n", week);
    printf("day: %d", day);

    return 0;
}
